<table class='table table-hover table-inverse'>
  <thead>
    <tr> 
      <th>会員id</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>

  <tbody>
    <% @end_users.each do |end_user| %>
      <tr>
        <td><%= end_user.id%></td>
        <td><%= end_user.last_name%><%= end_user.first_name%></td>
        <td><%= end_user.email%></td>
        <td><%= end_user.is_unsubscribed %></td>
      </tr>
    <% end %>
  </tbody>
</table>